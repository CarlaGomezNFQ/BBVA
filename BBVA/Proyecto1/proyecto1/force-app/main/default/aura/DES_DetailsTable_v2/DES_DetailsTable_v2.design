<design:component label="Details Table v2">
  <design:attribute name="TableHeading" label="Heading for the related table"
      default="Example of Table Heading" />
    <design:attribute name="fieldsToDisplay" label="Fields"
      required="true" />
  <design:attribute name="inNumberOfDecimals" label="Format Decimals"
      datasource="Unformatted,0,1,2"
      default="2" />
  <design:attribute name="cssClassName" label="The class name to format the table"
      datasource="Horizontal_Grey_Space,Horizontal_Grey_No_Space,Vertical_Clear,Vertical_Grey,Row,RowFields"
      default="Horizontal_Grey_No_Space" />
</design:component>